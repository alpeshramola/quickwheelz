{
  "name": "quickwheelz",
  "version": "1.0.0",
  "description": "A bike rental platform inspired by Royal Brothers",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "install-all": "npm install && cd client && npm install",
    "dev-all": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "razorpay": "^2.9.6",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5",
    "validator": "^13.15.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1"
  }
}
